<div class="container">
	<div class="row">
		<div class="col-md-4 col-md-offset-3" syle="margin-top:30px">
			<div id="form_wrapper">
			  <%= form_for(:session, url: login_path) do |f| %> 
			  	<h1>Username</h1>
			    <%= f.text_field :username, :placeholder => "Enter Username" %> <br>
			    <h1>Password</h1>
			    <%= f.password_field :password, :placeholder => "Enter Password" %> <br><br>
			    
			    <%= f.submit "Log in", class: "btn-submit" %>
			  <% end %>
			</div>
		</div>
	</div>
</div>